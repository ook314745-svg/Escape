    if keyBox.Text == correctKey then
        status.Text = " تم التفعيل بنجاح"
        status.TextColor3 = Color3.fromRGB(80, 220, 140)
        TweenService:Create(stroke, TweenInfo.new(0.4), {Color = Color3.fromRGB(80, 220, 140)}):Play()
