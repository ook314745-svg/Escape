local function getTargetForGap8()
    local map = Workspace:WaitForChild("DefaultMap", 10)
    if not map then return nil end
    local gapsFolder = map:WaitForChild("Gaps", 10)
    if not gapsFolder then return nil end
    local specificGap = gapsFolder:FindFirstChild("Gap8")
    if not specificGap then return nil end
    while #specificGap:GetChildren() < 2 do task.wait(0.1) end
    return specificGap:GetChildren()[2]
end

local function smoothTeleportTo(targetPart)
    local character = LocalPlayer.Character
    local rootPart = character and character:FindFirstChild("HumanoidRootPart")
    if not (rootPart and targetPart) then return end
    local targetPosition = targetPart.Position + Vector3.new(0, 5, 0)
    local tweenInfo = TweenInfo.new(0.1, Enum.EasingStyle.Linear)
    local properties = {CFrame = CFrame.new(targetPosition)}
    local tween = TweenService:Create(rootPart, tweenInfo, properties)
    tween:Play()
end

Gap8Button.MouseButton1Click:Connect(function()
    local target = getTargetForGap8()
    if target then
        smoothTeleportTo(target)
    else
        Gap8Button.Text = "لم يتم العثور على الهدف!"
        task.wait(2)
        Gap8Button.Text = "الانتقال لاخر الماب"
    end
end)



end})

