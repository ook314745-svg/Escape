local function fetchServers(cursor)
    local requestUrl = "https://games.roblox.com/v1/games/" .. placeId .. "/servers/Public?sortOrder=Asc&limit=100&cursor=" .. (cursor or "")
    local success, response = pcall(function()
        return HttpService:JSONDecode(game:HttpGet(requestUrl))
    end)
    if success and response and response.data then
        return response.data, response.nextPageCursor
    end
    return {}, nil
end

