NextButton.TextSize = 14

local StatusLabel = Instance.new("TextLabel")
StatusLabel.Parent = MainFrame
StatusLabel.Position = UDim2.new(0, 0, 1, -30)
StatusLabel.Size = UDim2.new(1, 0, 0, 25)
StatusLabel.Font = Enum.Font.SourceSansItalic
StatusLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
StatusLabel.BackgroundTransparency = 1
StatusLabel.TextXAlignment = Enum.TextXAlignment.Center


local totalGaps = 9
local currentGapIndex = 0


local function getTargetForIndex(index)
    local mapVariants = Workspace:WaitForChild("MapVariants")
    local map = mapVariants:WaitForChild("DefaultMap")
    local gapsFolder = map:WaitForChild("Gaps")
    
    local gapName = "Gap" .. index
    local specificGap = gapsFolder:WaitForChild(gapName)

    
    while #specificGap:GetChildren() < 3 do 
        wait(0.1)
    end
    
    if index == 1 then return specificGap:WaitForChild("Mud")
    elseif index == 2 then return specificGap:GetChildren()[2]
    elseif index == 3 then return specificGap:GetChildren()[2]
    elseif index == 4 then return specificGap:WaitForChild("Mud")
    elseif index == 5 then return specificGap:GetChildren()[2]
    elseif index == 6 then return specificGap:WaitForChild("Mud")
    elseif index == 7 then return specificGap:GetChildren()[3]
    elseif index == 8 then return specificGap:GetChildren()[2]
    elseif index == 9 then return specificGap:GetChildren()[3]
    end
    return nil
end

local function updateStatus()
    StatusLabel.Text = "سيف بوينت: " .. currentGapIndex .. " / " .. totalGaps
end

local function smoothTeleportTo(targetPart)
    local character = LocalPlayer.Character
    local rootPart = character and character:FindFirstChild("HumanoidRootPart")
    if not (rootPart and targetPart) then return end
    local targetPosition = targetPart.Position + Vector3.new(0, 5, 0)
    local tweenInfo = TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
    local properties = {CFrame = CFrame.new(targetPosition)}
    local tween = TweenService:Create(rootPart, tweenInfo, properties)
    tween:Play()
end

local function moveForward()
    if currentGapIndex < totalGaps then
        local nextIndex = currentGapIndex + 1
        local target = getTargetForIndex(nextIndex)
        if target then
            currentGapIndex = nextIndex
            smoothTeleportTo(target)
            updateStatus()
        end
    end
end

local function moveBackward()
    if currentGapIndex > 1 then
        local prevIndex = currentGapIndex - 1
        local target = getTargetForIndex(prevIndex)
        if target then
            currentGapIndex = prevIndex
            smoothTeleportTo(target)
            updateStatus()
        end
    end
end

local function resetOnDeath()
    currentGapIndex = 0
    updateStatus()
end

NextButton.MouseButton1Click:Connect(moveForward)
PrevButton.MouseButton1Click:Connect(moveBackward)

LocalPlayer.CharacterAdded:Connect(function(character)
    character:WaitForChild("Humanoid").Died:Connect(resetOnDeath)
end)

if LocalPlayer.Character then
    pcall(function()
        LocalPlayer.Character:WaitForChild("Humanoid").Died:Connect(resetOnDeath)
    end)
end


updateStatus()
end})

