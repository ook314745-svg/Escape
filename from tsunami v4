    if isFarming then
        setStatus("ÙŠØ§Ø¹ÙŠÙ†ÙŠ ØµØ§Ø­Ø¨Ù†Ø§ Ù…Ø§Øª-_- Ù‡Ù†Ø¹ÙŠØ¯ Ø§Ù„Ù…Ø­Ø§ÙˆÙ„Ø©")
    end
end)
