                local aIsRare = a.playing == 1 and a.ping > 300
                local bIsRare = b.playing == 1 and b.ping > 300
                if aIsRare ~= bIsRare then return aIsRare end
                


                if a.playing ~= b.playing then return a.playing < b.playing end
                

                
                return a.ping > b.ping
            end)

