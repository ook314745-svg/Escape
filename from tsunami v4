        while not foundStableServer and cursor ~= nil do
            statusLabel.Text = "Ø­Ø§Ù„ÙŠØ§ Ø¨Ù†Ø¯ÙˆØ± Ø§ØµØ¨Ø± ÙŠØ§ Ù…ØªØ³Ø±Ø¹... "
            local serverList, nextCursor = fetchServers(cursor)
