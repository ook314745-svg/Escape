local function updateSpeed(inputPos)
    local relativeX = math.clamp((inputPos.X - slider.AbsolutePosition.X) / slider.AbsoluteSize.X, 0, 1)
    speed = math.floor(1 + (499 * relativeX))
    fill.Size = UDim2.new(relativeX, 0, 1, 0)
    handleSlider.Position = UDim2.new(relativeX, -7, 0.5, -5)
    speedLabel.Text = "Speed: " .. speed
end

handleSlider.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        updateSpeed(input.Position)
        local moveConn, endConn
        moveConn = UserInputService.InputChanged:Connect(function(moveInput)
            if moveInput.UserInputType == Enum.UserInputType.MouseMovement or moveInput.UserInputType == Enum.UserInputType.Touch then
                updateSpeed(moveInput.Position)
            end
        end)
        endConn = UserInputService.InputEnded:Connect(function(endInput)
            if endInput.UserInputType == Enum.UserInputType.MouseButton1 or endInput.UserInputType == Enum.UserInputType.Touch then
                moveConn:Disconnect(); endConn:Disconnect()
            end
        end)
    end
end)
updateSpeed({X = slider.AbsolutePosition.X + (slider.AbsoluteSize.X * ((speed - 1) / 499))})


